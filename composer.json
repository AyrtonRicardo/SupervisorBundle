{
    "name": "yzalis/supervisor-bundle",
    "type": "symfony-bundle",
    "description": "Manage your supervisor instance in your Symfony2 application.",
    "keywords": ["supervisor", "supervisord", "supervisorctl", "rpc", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Benjamin Laugueux",
            "email": "benjamin@yzalis.com"
        },
        {
            "name": "All contributors",
            "homepage": "https://github.com/yzalis/SupervisorBundle/contributors"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "ext-json": "*",
        "supervisorphp/supervisor": "^v4.0",
        "php-http/client-common": "^2.3",
        "php-http/guzzle6-adapter": "^2.0",
        "php-http/message": "^1.11",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^4.1",
        "symfony/translation": "^4.1"
    },
    "autoload": {
        "psr-0": { "YZ\\SupervisorBundle": "" }
    },
    "target-dir": "YZ/SupervisorBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.x-dev"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
